CA,WAAA,KAAA,CAAA,IAAA,CAAA,sBAAA,EAAA;AACA,QAAA,KAAA,EAAA,KAAA,KADA;AAEA,QAAA,QAAA,EAAA,KAAA,QAFA;AAGA,QAAA,QAAA,EAAA,KAAA,QAHA;AAIA,QAAA,MAAA,EAAA,KAAA;AAJA,OAAA,EAKA,IALA,CAKA,UAAA,GAAA,EAAA;AACA,YAAA,MAAA,GAAA,GAAA,CAAA,IAAA,CAAA,MAAA;AACA,YAAA,IAAA,GAAA,KAAA,CAFA,CAEA;;AACA,YAAA,MAAA,KAAA,CAAA,EAAA;AACA,UAAA,UAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,MAFA;AAGA,YAAA,EAAA,EAAA,IAHA;AAIA,YAAA,QAJA,sBAIA;AACA,cAAA,IAAA,CAAA,OAAA,CAAA,IAAA,CAAA,aAAA;AACA;AANA,WAAA,CAAA;AAQA,SATA,MAUA;AACA,UAAA,UAAA,CAAA;AACA,YAAA,KAAA,EAAA,IADA;AAEA,YAAA,OAAA,EAAA,GAAA,CAAA,IAAA,CAAA,GAAA,GAAA,QAFA;AAGA,YAAA,EAAA,EAAA;AAHA,WAAA,CAAA;AAKA;AACA,OAzBA;AA2BA;AAhDA;AAVA,CAAA","sourcesContent":["<template>\r\n    <div class=\"register_body\">\r\n        <div class=\"register_email\">\r\n            邮箱：<input v-model=\"email\" class=\"register_text\" type=\"text\"> <button @touchstart=\"handleToVerify()\">发送验证码</button>\r\n        </div>\r\n\r\n        <div>\r\n            用户名:<input v-model=\"username\" class=\"register_text\" type=\"text\">\r\n        </div>\r\n\r\n        <div>\r\n            密码:<input v-model=\"password\" class=\"register_text\" type=\"text\">\r\n        </div>\r\n\r\n        <div>\r\n            确认密码:<input class=\"register_text\" type=\"text\">\r\n        </div>\r\n\r\n        <div>\r\n            验证码:<input v-model=\"verify\" class=\"register_text\" type=\"text\">\r\n        </div>\r\n\r\n        <div class=\"register_btn\">\r\n            <button @touchstart=\"handleToRegister()\">注册</button>\r\n        </div>\r\n        <div class=\"register_link\">\r\n            <router-link to=\"/mine/login\">立即登录</router-link>\r\n            <router-link to=\"/mine/findPassword\">找回密码</router-link>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport {messageBox} from '@/components/JS'\r\nexport default {\r\n    name : 'register',\r\n    data(){\r\n        return{\r\n            email : '',\r\n            username : '',\r\n            password : '',\r\n            verify : '',\r\n        }\r\n    },\r\n    methods : {\r\n        handleToVerify(){\r\n            this.axios.get('/api2/users/verify?email='+this.email).then((res)=>{\r\n                var status = res.data.status;\r\n                if(status === 0){\r\n                    messageBox({\r\n                        title : '验证码',\r\n                        content : '验证码已发送',\r\n                        ok : '确定'\r\n                    });\r\n                }\r\n                else{\r\n                    messageBox({\r\n                        title : '验证码',\r\n                        content : '验证码发送失败',\r\n                        ok : '确定'\r\n                    });\r\n                }\r\n            })\r\n        },\r\n        handleToRegister(){\r\n            this.axios.post('/api2/users/register',{\r\n                email : this.email,\r\n                username : this.username,\r\n                password : this.password,\r\n                verify : this.verify\r\n            }).then((res)=>{\r\n                var status = res.data.status;\r\n                var This = this;//跳转路由\r\n                if(status === 0){\r\n                    messageBox({\r\n                        title : '注册',\r\n                        content : '注册成功',\r\n                        ok : '确定',\r\n                        handleOk(){\r\n                            This.$router.push('/mine/login')\r\n                        }\r\n                    });\r\n                }\r\n                else{\r\n                    messageBox({\r\n                        title : '注册',\r\n                        content : res.data.msg + ',请重新注册',\r\n                        ok : '确定'\r\n                    });\r\n                }\r\n            });\r\n            \r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n#content .register_body{  width:100%;}\r\n.register_body .register_text{ width:100%; height: 40px; border:none; border-bottom: 1px #ccc solid; margin-bottom: 5px; outline: none; text-indent: 10px;}\r\n.register_body .register_btn{ height:50px; margin:10px;}\r\n.register_body .register_btn{position: relative;}\r\n.register_body .register_email button{position: absolute; right: 10px;top: 10px;height: 30px;}\r\n.register_body .register_btn button{ width:100%; height:100%; background:#e54847; border-radius: 3px; border:none; display: block; color:white; font-size: 20px;}\r\n.register_body .register_link{ display: flex; justify-content:space-between;}\r\n.register_body .register_link a{ text-decoration: none; margin:0 5px; font-size: 12px; color:#e54847;}\r\n</style>"],"sourceRoot":"src/components/Register"}]}